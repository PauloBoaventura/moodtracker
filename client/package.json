{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:js": "CACHE_LIST=CACHE_LIST parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "prettier --write '**/*'",
    "icons": "node scripts/icons",
    "start": "parcel src/index.html",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level critical || [ $? -lt 16 ]",
    "test:e2e": "jest e2e",
    "test:fmt": "prettier --check '**/*'",
    "test:types": "tsc --noEmit",
    "test:unit": "jest src"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "amazon-cognito-identity-js": "^4.3.3",
    "date-fns": "^2.15.0",
    "eri": "^19.0.1",
    "idb-keyval": "^3.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regression": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/puppeteer": "^3.0.1",
    "@types/reach__router": "^1.3.4",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/regression": "^2.0.0",
    "favicons": "^6.1.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "puppeteer": "^5.1.0",
    "ts-jest": "^26.1.2",
    "typescript": "^4.0.2"
  }
}
