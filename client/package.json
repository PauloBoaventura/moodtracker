{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:js": "CACHE_LIST=CACHE_LIST parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "prettier --write '**/*'",
    "icons": "node scripts/icons",
    "start": "parcel src/index.html --open",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level critical || [ $? -lt 16 ]",
    "test:e2e": "jest e2e",
    "test:fmt": "prettier --check '**/*'",
    "test:types": "tsc --noEmit",
    "test:unit": "TZ=UTC jest src"
  },
  "browserslist": [
    ">1.25%"
  ],
  "dependencies": {
    "@reach/router": "^1.3.3",
    "@reduxjs/toolkit": "^1.4.0",
    "amazon-cognito-identity-js": "^4.5.2",
    "chroma-js": "^2.1.0",
    "date-fns": "^2.15.0",
    "eri": "^24.0.0",
    "idb-keyval": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.0",
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^3.0.4",
    "@types/reach__router": "^1.3.4",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.9",
    "favicons": "^6.1.0",
    "jest": "^26.6.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "puppeteer": "^5.4.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.2"
  }
}
