{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:js": "CACHE_LIST=CACHE_LIST parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "prettier --write '**/*'",
    "icons": "node scripts/icons",
    "start": "parcel src/index.html",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level high || [ $? -lt 8 ]",
    "test:fmt": "prettier --check '**/*'",
    "test:types": "tsc --noEmit",
    "test:unit": "jest"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "amazon-cognito-identity-js": "^4.3.1",
    "d3-scale-chromatic": "^1.5.0",
    "eri": "^18.2.3",
    "idb-keyval": "^3.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/d3-scale-chromatic": "^1.5.0",
    "@types/jest": "^26.0.0",
    "@types/reach__router": "^1.3.4",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "favicons": "^6.1.0",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  }
}
